<?xml version="1.0" encoding="utf-8"?>
<style xmlns="http://purl.org/net/xbiblio/csl" version="1.0">
  <info>
    <title>Custom Vancouver with full authors (limit 10) and DOI</title>
    <id>http://www.zotero.org/styles/custom-vancouver-author-limit</id>
    <link href="http://www.zotero.org/styles/vancouver" rel="self"/>
    <author>
      <name>ChatGPT Custom CSL Generator</name>
    </author>
    <updated>2025-06-11T00:00:00+00:00</updated>
    <category citation-format="numeric"/>
    <summary>Vancouver style with full author list (limit 10 authors, then '...' and last author), full title, abbreviated journal, year, volume, pages, and DOI with period.</summary>
  </info>

  <macro name="author">
    <names variable="author">
      <name form="long" and="text" delimiter=", "/>
      <label form="short" prefix=", " suffix="."/>
      <et-al min="11" use-first="10" subsequent-author-substitute="..." delimiter=", " />
    </names>
  </macro>

  <macro name="title">
    <text variable="title" suffix=". "/>
  </macro>

  <macro name="journal">
    <group delimiter=" ">
      <text variable="container-title" form="short"/>
      <text variable="volume" font-weight="bold"/>
      <text variable="page"/>
    </group>
